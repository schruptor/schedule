{
    "name": "schruptor/schedule",
    "description": "This package gives your users an overview what you scheduled jobs are.",
    "keywords": [
        "schruptor",
        "laravel",
        "schedule"
    ],
    "homepage": "https://github.com/schruptor/schedule",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Fl√ºgge",
            "email": "schedule@relegate.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "laravel/framework": "7.*|8.*|9.*"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Schruptor\\Schedule\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schruptor\\Schedule\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schruptor\\Schedule\\ScheduleServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
